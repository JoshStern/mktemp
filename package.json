    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src",
    "test": "npm run lint && jest"
  },
  "keywords": [
    "temp",
    "temporary",
    "tmp",
    "dir",
    "directory",
    "files",
    "async",
    "promise",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JoshStern/mktemp"
  },
  "author": "Josh Stern <josh.stern95@gmail.com>",
