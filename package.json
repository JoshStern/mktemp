  "bugs": {
    "url": "https://github.com/JoshStern/mktemp/issues"
  },
  "homepage": "https://mktemp.jpastern.dev",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "typedoc": "^0.22.11",
    "typedoc-plugin-cname": "^1.0.1",
    "typescript": "^4.1.4"
