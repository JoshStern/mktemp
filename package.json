{
  "name": "@josh_stern/mktemp",
  "version": "1.0.4",
  "description": "Async temporary files for node written in Typescript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
